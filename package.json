{
	"name": "@limbo-chat/openai-utils",
	"type": "module",
	"description": "OpenAI utilities for Limbo plugins",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"name": "Wes Lowe",
		"email": "wesley.jlowe10@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/limbo-chat/openai-utils"
	},
	"keywords": [
		"limbo",
		"llm",
		"ai"
	],
	"scripts": {
		"build": "tsup",
		"typecheck": "tsc --noEmit"
	},
